import sqlite3

conn = sqlite3.connect('tutorial.db')  # 在colab產生的資料庫檔, 可直接在colab文件刷新後看到, 而後mouse點兩下即可下載
c = conn.cursor()
def create_table():
    c.execute("CREATE TABLE IF NOT EXISTS stuffToPlot(unix REAL, datestamp TEXT, keyword TEXT, value REAL)")

def data_entry():
    c.execute("INSERT INTO stuffToPlot VALUES(1452549219,'2016-01-11 13:53:39','Python',6)")

    conn.commit()
    c.close()
    conn.close()
    
create_table()
data_entry()
